{
    "extends": "airbnb-base",
    "plugins": [
        "import"
    ],
    "rules": {
        "indent": 0,
        "key-spacing": 0,
        "arrow-parens": 0,
        "no-plusplus": 0,
        "comma-dangle": ["error", {
            "arrays": "always",
            "objects": "always-multiline",
            "imports": "ignore",
            "exports": "ignore",
            "functions": "ignore"
        }],

        "no-unused-vars": 0,
        "no-undef": 0
    }
}